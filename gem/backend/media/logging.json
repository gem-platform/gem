{
    "version": 1,
    "formatters": { 
        "f": { "format": "%(name)-14s %(levelname)-8s %(message)s" }
    },
    "handlers": {
        "h": {
            "class": "logging.StreamHandler",
            "formatter": "f",
            "level": "DEBUG"
        }
    },
    "loggers": {
        "communication": {
            "handlers": ["h"],
            "level": "DEBUG"
        },
        "meetings": {
            "handlers": ["h"],
            "level": "DEBUG"
        },
        "sessions": {
            "handlers": ["h"],
            "level": "DEBUG"
        },
        "root": {
            "handlers": ["h"],
            "level": "DEBUG"
        }
    }
}
